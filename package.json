    "<<<<<<Both>>>>>>": "",
    "start": "APP_CONFIG_PATH='../app-config.json' concurrently \"pnpm start:client\" \"pnpm start:server\"",
    "build": "APP_CONFIG_PATH='../app-config.json' concurrently \"pnpm build:client\" \"pnpm build:server\"",
    "serve": "APP_CONFIG_PATH='../app-config.json' pnpm serve:server"
  },
