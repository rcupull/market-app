{
  "name": "app",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "<<<<<<Web>>>>>>": "",
    "start:client": "cd ./web && pnpm start",
    "build:client": "cd ./web && pnpm build",
    "<<<<<<Backend>>>>>>": "",
    "start:server": "cd ./back && pnpm start",
    "build:server": "cd ./back && pnpm build",
    "serve:server": "cd ./back && pnpm serve",
    "<<<<<<Both>>>>>>": "",
    "start": "NODE_ENV=development DB_ENV=development concurrently \"pnpm start:client\" \"pnpm start:server\"",
    "start:prod": "NODE_ENV=development DB_ENV=production concurrently \"pnpm start:client\" \"pnpm start:server\"",
    "stage": "NODE_ENV=development DB_ENV=development concurrently \"pnpm build:client\" \"pnpm build:server\" && pnpm serve",
    "build": "NODE_ENV=production DB_ENV=production concurrently \"pnpm build:client\" \"pnpm build:server\"",
    "serve": "NODE_ENV=development DB_ENV=development pnpm serve:server"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
