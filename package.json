{
  "name": "app",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "<<<<<<Web>>>>>>": "",
    "start:client": "cd ./web && pnpm start",
    "lint:fix:client": "cd ./web && pnpm lint:fix",
    "tsc:client": "cd ./web && pnpm tsc",
    "build:client": "cd ./web && pnpm build",
    "install:client": "cd ./web && pnpm install",
    "<<<<<<Backend>>>>>>": "",
    "start:server": "cd ./back && pnpm start",
    "lint:fix:server": "cd ./back && pnpm lint:fix",
    "tsc:server": "cd ./back && pnpm tsc",
    "build:server": "cd ./back && pnpm build",
    "serve:server": "cd ./back && pnpm serve",
    "install:server": "cd ./back && pnpm install",
    "<<<<<<CKEDITOR>>>>>>": "",
    "ready:ckeditor": "cd ./ckeditor5-build && pnpm install && pnpm build",
    "<<<<<<Both>>>>>>": "",
    "start": "env-cmd -e start concurrently \"pnpm start:client\" \"pnpm start:server\"",
    "lint:fix": "env-cmd -e start concurrently \"pnpm lint:fix:client\" \"pnpm lint:fix:server\"",
    "tsc": "env-cmd -e start concurrently \"pnpm tsc:client\" \"pnpm tsc:server\"",
    "start:prod": "env-cmd -e start:prod concurrently \"pnpm start:client\" \"pnpm start:server\"",
    "stage": "env-cmd -e stage concurrently \"pnpm build:client\" \"pnpm build:server\" && pnpm serve",
    "build": "env-cmd -e build concurrently \"pnpm build:client\" \"pnpm build:server\"",
    "install:all": "pnpm ready:ckeditor && concurrently \"pnpm install:client\" \"pnpm install:server\"",
    "serve": "env-cmd -e serve pnpm serve:server"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "env-cmd": "^10.1.0"
  }
}
